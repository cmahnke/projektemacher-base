{{- $labels := dict "en" (slice "Projektemacher") "de" (slice "Projektemacher") -}}
"provider": [
  {
    "id": "https://projektemacher.org/",
    "type": "Agent",
    "label": {{ $labels | jsonify }},
    {{- $homepage := dict "id" "https://projektemacher.org/" "type" "Text" "label" $labels -}}
    "homepage": [
      {
        "id": "https://projektemacher.org/",
        "type": "Text",
        "label": {{ $labels | jsonify }},
        "format": "text/html"
    }
    {{- $provider := dict "provider" (slice (dict "id" "https://projektemacher.org/" "type" "Agent" "label" $labels "homepage" (slice $homepage))) -}}
  ],
  "logo": [
    {
      {{- if isset .Params "iiifLogo" -}}
        {{- errorf "[iiif/3/provider.json] .Params.iiifLogo isn't supported anymore use .Params.iiif.logo" -}}
      {{- end -}}
      {{- if and (isset .Params "iiif") (isset .Params.iiif "logo") -}}
        {{- warnf "[iiif/3(provider.json] Using provided logo - check if this works as expected" -}}
        "id":"{{ .Params.iiif.logo.img }}",
        "service":{
           "@context":"http://iiif.io/api/image/2/context.json",
           "id":"{{ .Params.iiif.logo.url }}",
           "profile":"http://iiif.io/api/image/2/level0.json"
        }
      {{- else -}}
        "id": "https://projektemacher.org/images/logo.png",
        "type": "Image",
        "format": "image/png",
        "height": 1024,
        "width": 1024
      {{- end -}}
    }
  ],
  {{- $seeAlso := dict "seeAlso" (slice (dict "id" "https://projektemacher.org/projektemacher.json" "type" "Dataset" "format" "application/ld+json" "profile" "https://schema.org/")) -}}
  "seeAlso": [
    {
      "id": "https://projektemacher.org/projektemacher.json",
      "type": "Dataset",
      "format": "application/ld+json",
      "profile": "https://schema.org/"
    }
  ]
  }
]
