{{- $labels := dict "en" (slice "Projektemacher") "de" (slice "Projektemacher") -}}

{{- $homepage := dict "id" "https://projektemacher.org/" "type" "Text" "label" $labels "format" "text/html" -}}
{{- $provider := dict "id" "https://projektemacher.org/" "type" "Agent" "label" $labels "homepage" (slice $homepage) -}}
{{- $logo := slice -}}
{{- if and (isset .Params "iiif") (isset .Params.iiif "logo") -}}
  {{- $logo = $logo | append (dict "id" .Params.iiif.logo.img "service" (dict "@context" "http://iiif.io/api/image/2/context.json" "id" .Params.iiif.logo.url "profile" "http://iiif.io/api/image/2/level0.json")) -}}
{{- else -}}
  {{- $logo = $logo | append (dict "id" "https://projektemacher.org/images/logo.png" "type" "Image" "format" "image/png" "height" 1024 "width" 1024) -}}
{{- end -}}
{{- $provider = $provider | merge (dict "logo" $logo) -}}
{{- $seeAlso := slice (dict "id" "https://projektemacher.org/projektemacher.json" "type" "Dataset" "format" "application/ld+json" "profile" "https://schema.org/") -}}
{{- $provider = $provider | merge (dict "seeAlso" $seeAlso) -}}

{{- return slice $provider -}}
